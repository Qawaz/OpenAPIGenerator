@partial_raw

@function getHtmlType(param)
    @if(@var(param.getType().startsWith("object")))
        @if(param.contains("__map_of__"))
            @return "map&lt;string, " + getHtmlType(param.__map_of__) + "&gt;"
        @else
            @return param.getName()
        @endif
    @elseif(@var(param.getType().startsWith("list")))
        @return "list&lt;" + getHtmlType(param[0]) + "&gt;"
    @elseif(@var(param.getType().startsWith("mutable_list")))
        @return "list&lt;" + getHtmlType(param[0]) + "&gt;"
    @else
        @return param.getType()
    @endif
@end_function

@end_partial_raw